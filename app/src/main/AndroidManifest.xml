<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="radiant.rpl.radiantrpl">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/testalogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/testalogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.radiant.rpl.testa.Reverify"
            android:theme="@style/AppTheme1"
            ></activity>
        <activity android:name="com.radiant.rpl.testa.MainActivity"
            android:theme="@style/AppTheme1"
            />
        <activity android:name="com.radiant.rpl.testa.Registration_Done" />
        <activity
            android:name="com.radiant.rpl.testa.Start_Registration"
            android:theme="@style/AppTheme1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>